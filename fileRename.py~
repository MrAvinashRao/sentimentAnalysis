import os;
#dir_list =["neg","pos"];
file_list = os.listdir("neg");
#file_list = os.listdir("pos");
num = 1; 
for k in file_list:
	newname = "neg/Rev"+str(num)+".txt"
	#print(newname);
	oldname = "neg/"+k;
	#print(oldname); 
	os.rename(oldname,newname);
	num = num +1;
#print(file_list.sort());






